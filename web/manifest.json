{
    "manifest_version": 2,
  "name": "chrome plugin",
  "description": "pepega",
  "version": "1.0",
  "content_scripts":[
      {
          // might want to replace with google meets
        "matches":[
            "<all_urls>" 
          ],
        "js": ["content.js"]
      }
    ],
  "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
  "background": {
        "scripts": ["background.js"],
        "persistent": false
      },
  "permissions": [
      "activeTab",
      "storage",
      "https://ajax.googleapis.com/"
    ]
  }